@article{Riches_1995, 
title={Hunter-Gatherer Structural Transformations}, 
volume={1}, 
ISSN={13590987, 14679655}, 
DOI={10.2307/3034956}, 
abstractNote={[A methodology for addressing structural transformation is illustrated with material from hunter-gatherer societies. A proper account of the transformation between types of social structure is in three basic steps. First, a separate, full analysis for each type is required, in order to discern which among its array of distinctive institutions is generatively fundamental; it is upon this fundamental institution that the transformation is focused. Secondly, an examination of the principles of structuring exemplified in each type is needed; this permits the question of legitimacy relating to the transformation to be addressed. Finally, the cultural change which underpins the transformation should be explained. Three types of hunter-gatherer social structure are used - Eskimo/Bushman, Northwest Coast Indian and Australian. Particular attention is paid to the logic of the social structure among the Australian, and to revealing the contrasting principles of structuring that pertain to the three types. Eskimo/Bushman institutions are shown to be informed more by `information structuring’, whilst Northwest Coast Indian and Australian are informed more by `authority structuring’; in terms of legitimacy this is a radical difference.]}, 
number={4}, 
journal={The Journal of the Royal Anthropological Institute},
publisher={[Wiley, Royal Anthropological Institute of Great Britain and Ireland]}, 
author={Riches, David},
year={1995}, 
pages={679–701} }


